{
  "manifest_version": 3,
  "name": "Timo工具箱",
  "version": "1.0.0",
  "description": "Timo工具箱",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_icon": "icon.png",
    "default_title": "Timo工具箱",
    "default_popup": "index.html"
  },
  "homepage_url": "https://www.feishu.cn/invitation/page/add_contact/?token=314s193d-15aa-4d9f-beed-34890838c767&amp;unique_id=15gELVC3fwqBJpSv-Z1vNw==",
  "permissions": ["contextMenus", "tabs", "bookmarks", "webRequest", "storage", "scripting"],
  "host_permissions": ["*://*/*"],
  "omnibox": { "keyword": "fe" },
  "background": {
    "service_worker": "scripts/backgroundV3.js"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "<all_urls>"],
      "js": ["scripts/content-script-v3.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["page.js"],
      "matches": [
        "http://bpo.fcash.com:1024/*",
        "http://admin.test.hw-touchme.com/*",
        "http://admin2.test.hw-touchme.com/*",
        "http://admin3.test.hw-touchme.com/*",
        "https://admin-gray.touchchat.me/*",
        "https://admin.touchchat.me/*"
      ]
    },
    {
      "resources": ["feishu.js"],
      "matches": ["*://*.feishu.cn/*"]
    }
  ]
}
